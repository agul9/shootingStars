void updateAlien() {

 if (alien.collisionTimer > 0) {
  alien.collisionTimer--;
 }


 if ((~((*(volatile unsigned short *)0x04000130)) & ((1<<5)))
  && alien.col >= alien.cdel) {

  alien.col -= alien.cdel;

 } else if ((~((*(volatile unsigned short *)0x04000130)) & ((1<<4)))
  && alien.col + alien.width - 1 < 240 - alien.cdel) {

  alien.col += alien.cdel;

 } else if ((~((*(volatile unsigned short *)0x04000130)) & ((1<<6))) && alien.row > 120) {
  alien.row -= alien.cdel;
 } else if ((~((*(volatile unsigned short *)0x04000130)) & ((1<<7))) && alien.row < 140) {
  alien.row += alien.cdel;
 }


 if ((!(~(oldButtons)&((1<<1))) && (~buttons & ((1<<1)))) && alien.bulletTimer >= 20) {
   fireBullet();
  alien.bulletTimer = 0;
 }
 alien.bulletTimer++;


 if (timer%2 == 0) {
  fallingStars();
 }
 timer++;
}


